                        <crop
                            jcr:primaryType="nt:unstructured"
                            features="*">
                            <aspectRatios jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    name="Wide Landscape"
                                    ratio="0.6180"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    name="Landscape"
                                    ratio="0.8284"/>
                                <item2
                                    jcr:primaryType="nt:unstructured"
                                    name="Square"
                                    ratio="1"/>
                                <item3
                                    jcr:primaryType="nt:unstructured"
                                    name="Portrait"
                                    ratio="1.6180"/>
                            </aspectRatios>
                        </crop>
                        <rotate
